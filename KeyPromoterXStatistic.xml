<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'Strg+Alt+A'</ShortCut>
          <Description>Add</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Strg+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Einfg' or 'Strg+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Start'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Umschalt+F10'</ShortCut>
          <Description>Run 'Start'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'start'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Umschalt+F10'</ShortCut>
          <Description>Run 'start'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Start'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Strg+F2'</ShortCut>
          <Description>Stop 'Start'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>